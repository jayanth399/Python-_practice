    "# CELL: robust_arff_fallback_loader\n",
    "from pathlib import Path\n",
    "import pandas as pd\n",
    "import io\n",
    "\n",
    "raw_dir = Path('../data')\n",
    "\n",
    "# standard column names (NSL-KDD)\n",
    "columns = [\n",
    "    'duration','protocol_type','service','flag','src_bytes','dst_bytes','land',\n",
    "    'wrong_fragment','urgent','hot','num_failed_logins','logged_in','num_compromised',\n",
    "    'root_shell','su_attempted','num_root','num_file_creations','num_shells',\n",
    "    'num_access_files','num_outbound_cmds','is_host_login','is_guest_login','count',\n",
    "    'srv_count','serror_rate','srv_serror_rate','rerror_rate','srv_rerror_rate',\n",
    "    'same_srv_rate','diff_srv_rate','srv_diff_host_rate','dst_host_count',\n",
    "    'dst_host_srv_count','dst_host_same_srv_rate','dst_host_diff_srv_rate',\n",
    "    'dst_host_same_src_port_rate','dst_host_srv_diff_host_rate','dst_host_serror_rate',\n",
    "    'dst_host_srv_serror_rate','dst_host_rerror_rate','dst_host_srv_rerror_rate','label'\n",
    "]\n",
    "\n",
    "def load_after_data_section(path):\n",
    "    \"\"\"Load an ARFF-like file by skipping header up to @data and parsing the rest.\"\"\"\n",
    "    txt = path.read_text(errors='ignore')\n",
    "    # find @data line (case-insensitive)\n",
    "    idx = txt.lower().find('@data')\n",
    "    if idx == -1:\n",
    "        # maybe file starts directly with rows (no header)\n",
    "        data_text = txt\n",
    "    else:\n",
    "        data_text = txt[idx + len('@data'):]  # everything after @data\n",
    "    # strip leading blank lines\n",
    "    data_text = data_text.lstrip(\"\\r\\n \")\n",
    "    # remove curly braces or weird quotes around tokens (common in some ARFFs)\n",
    "    # But be careful not to remove commas inside tokens; we'll clean tokens later\n",
    "    # Try several separators: comma, tab, whitespace\n",
    "    for sep in [',','\\t','\\\\s+']:\n",
    "        try:\n",
    "            # use engine='python' when regex separator used\n",
    "            if sep == '\\\\s+':\n",
    "                df = pd.read_csv(io.StringIO(data_text), header=None, sep=r'\\s+', engine='python', quoting=3, na_filter=False)\n",
    "            else:\n",
    "                df = pd.read_csv(io.StringIO(data_text), header=None, sep=sep, engine='python', quoting=3, na_filter=False)\n",
    "            # quick sanity: should have 42 columns for NSL-KDD (41 features + label)\n",
    "            if df.shape[1] >= 41:  # allow >= because sometimes extra empty col appears\n",
    "                # if more columns than expected, keep first len(columns)\n",
    "                if df.shape[1] > len(columns):\n",
    "                    df = df.iloc[:, :len(columns)]\n",
    "                df.columns = columns[:df.shape[1]]\n",
    "                return df\n",
    "        except Exception as e:\n",
    "            # try next separator\n",
    "            #print(\"sep\", sep, \"failed:\", e)\n",
    "            pass\n",
    "    raise ValueError(f\"Could not parse data section of {path.name} with common separators.\")\n",
    "\n",
    "# Try train/test files (fall back to .txt if .arff fails)\n",
    "def find_file(base):\n",
    "    for ext in ['.arff','.txt']:\n",
    "        p = raw_dir / (base + ext)\n",
    "        if p.exists():\n",
    "            return p\n",
    "    # fallback any matching\n",
    "    for p in raw_dir.glob(base + '*'):\n",
    "        return p\n",
    "    return None\n",
    "\n",
    "train_path = find_file('KDDTrain+')\n",
    "test_path  = find_file('KDDTest+')\n",
    "if not train_path or not test_path:\n",
    "    raise FileNotFoundError(\"Could not find KDDTrain+ or KDDTest+ in data/\")\n",
    "\n",
    "print(\"Loading train from:\", train_path.name)\n",
    "df_train = load_after_data_section(train_path)\n",
    "print(\"Loaded train shape:\", df_train.shape)\n",
    "\n",
    "print(\"Loading test from:\", test_path.name)\n",
    "df_test = load_after_data_section(test_path)\n",
    "print(\"Loaded test shape:\", df_test.shape)\n",
    "\n",
    "# Save CSVs\n",
    "train_csv = raw_dir / 'nsl_kdd_train.csv'\n",
    "test_csv  = raw_dir / 'nsl_kdd_test.csv'\n",
    "df_train.to_csv(train_csv, index=False)\n",
    "df_test.to_csv(test_csv, index=False)\n",
    "print(\"Saved:\", train_csv.name, test_csv.name)\n"
   ]
