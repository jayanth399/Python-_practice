    "# CELL: risk_series_and_forecast\n",
    "import pandas as pd\n",
    "from sklearn.ensemble import GradientBoostingRegressor\n",
    "from sklearn.metrics import mean_squared_error\n",
    "from sklearn.model_selection import train_test_split\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# Load test CSV (full) and use ground-truth label column to make risk time series\n",
    "df_test = pd.read_csv('../data/nsl_kdd_test.csv')\n",
    "# Make binary label\n",
    "df_test['label_binary'] = df_test['label'].apply(lambda x: 0 if str(x).lower()=='normal' else 1)\n",
    "\n",
    "# Split into windows (e.g., 200 windows)\n",
    "N_WINDOWS = 200\n",
    "bins = np.array_split(df_test, N_WINDOWS)\n",
    "risk_series = pd.Series([b['label_binary'].mean() for b in bins], name='true_risk')\n",
    "\n",
    "# Build lag features\n",
    "lags = 3\n",
    "Xf = pd.DataFrame({f'lag{i}': risk_series.shift(i) for i in range(1, lags+1)})\n",
    "Xf = Xf.dropna()\n",
    "yf = risk_series.loc[Xf.index]\n",
    "\n",
    "# Train/test split without shuffling (time series)\n",
    "split_idx = int(len(Xf)*0.8)\n",
    "Xf_train, Xf_test = Xf.iloc[:split_idx], Xf.iloc[split_idx:]\n",
    "yf_train, yf_test = yf.iloc[:split_idx], yf.iloc[split_idx:]\n",
    "\n",
    "reg = GradientBoostingRegressor()\n",
    "reg.fit(Xf_train, yf_train)\n",
    "yp = reg.predict(Xf_test)\n",
    "\n",
    "mse = mean_squared_error(yf_test, yp)\n",
    "rmse = np.sqrt(mse)\n",
    "print(f\"Forecast RMSE: {rmse:.6f}\")\n",
    "\n",
    "# Plot\n",
    "plt.figure(figsize=(8,4))\n",
    "plt.plot(range(len(yf_test)), yf_test.values, label='true')\n",
    "plt.plot(range(len(yp)), yp, label='pred')\n",
    "plt.legend()\n",
    "plt.title('True vs Predicted disruption risk (test windows)')\n",
    "plt.show()\n"
   ]
  },
