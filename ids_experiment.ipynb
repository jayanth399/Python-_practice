    "# CELL 3: preprocessing\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "from sklearn.preprocessing import LabelEncoder, StandardScaler\n",
    "from pathlib import Path\n",
    "\n",
    "raw = Path('../data')\n",
    "df = pd.read_csv(raw/'nsl_kdd_train_small.csv')  # or use nsl_kdd_train.csv for full run\n",
    "\n",
    "# Binary label: normal=0, attack=1\n",
    "df['label_binary'] = df['label'].apply(lambda x: 0 if str(x).lower()=='normal' else 1)\n",
    "\n",
    "# Categorical columns to encode\n",
    "cat_cols = ['protocol_type','service','flag']\n",
    "for c in cat_cols:\n",
    "    df[c] = df[c].astype(str)\n",
    "\n",
    "# Label encode categorical columns\n",
    "le_dict = {}\n",
    "for c in cat_cols:\n",
    "    le = LabelEncoder()\n",
    "    df[c] = le.fit_transform(df[c])\n",
    "    le_dict[c] = le\n",
    "\n",
    "# Identify numeric columns (exclude original label and new label_binary)\n",
    "exclude = set(['label','label_binary'])\n",
    "num_cols = [c for c in df.columns if c not in list(exclude) + cat_cols]\n",
    "\n",
    "# Coerce to numeric, fillna\n",
    "for col in num_cols:\n",
    "    df[col] = pd.to_numeric(df[col], errors='coerce').fillna(0)\n",
    "\n",
    "# Scale numeric features\n",
    "scaler = StandardScaler()\n",
    "df[num_cols] = scaler.fit_transform(df[num_cols])\n",
    "\n",
    "# Final X, y\n",
    "X = df.drop(columns=['label','label_binary']).copy()\n",
    "y = df['label_binary'].values\n",
    "\n",
    "print(\"Preprocessing done. X shape:\", X.shape, \"y shape:\", y.shape)\n"
   ]
