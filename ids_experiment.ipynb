    "# CELL: risk_series_and_forecast_debug (replace old Step 5)\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "from pathlib import Path\n",
    "from sklearn.ensemble import GradientBoostingRegressor\n",
    "from sklearn.metrics import mean_squared_error\n",
    "import matplotlib.pyplot as plt\n",
    "import traceback\n",
    "\n",
    "raw = Path('../data')\n",
    "test_csv = raw / 'nsl_kdd_test.csv'\n",
    "\n",
    "print(\"Checking test CSV:\", test_csv, \"exists?\", test_csv.exists())\n",
    "if not test_csv.exists():\n",
    "    raise FileNotFoundError(f\"{test_csv} not found. Make sure nsl_kdd_test.csv is in Desktop/ids_project/data/\")\n",
    "\n",
    "# Load test CSV with safe options\n",
    "try:\n",
    "    df_test = pd.read_csv(test_csv, low_memory=False)\n",
    "except Exception as e:\n",
    "    print(\"Error loading CSV with pandas.read_csv:\")\n",
    "    traceback.print_exc()\n",
    "    raise\n",
    "\n",
    "print(\"Raw test shape:\", df_test.shape)\n",
    "print(\"Columns (first 50):\", df_test.columns.tolist()[:50])\n",
    "\n",
    "# Ensure label column exists and convert to binary\n",
    "label_col_candidates = [c for c in df_test.columns if 'label' in c.lower()]\n",
    "print(\"Label column candidates found:\", label_col_candidates)\n",
    "if len(label_col_candidates) == 0:\n",
    "    raise KeyError(\"No 'label' column found in test CSV. Check header names.\")\n",
    "label_col = label_col_candidates[0]\n",
    "print(\"Using label column:\", label_col)\n",
    "\n",
    "# Normalize label values and create label_binary\n",
    "def to_binary_label(x):\n",
    "    try:\n",
    "        s = str(x).strip().lower()\n",
    "        return 0 if s == 'normal' else 1\n",
    "    except Exception:\n",
    "        return 1\n",
    "\n",
    "df_test['label_binary'] = df_test[label_col].apply(to_binary_label)\n",
    "print(\"label_binary value counts:\\n\", df_test['label_binary'].value_counts())\n",
    "\n",
    "# Decide number of windows based on test rows (avoid empty windows)\n",
    "n_rows = len(df_test)\n",
    "# choose N_WINDOWS such that average rows per window >= 20 to avoid many empty windows\n",
    "N_WINDOWS = min(200, max(10, n_rows // 20))\n",
    "print(f\"n_rows={n_rows}, using N_WINDOWS={N_WINDOWS} (avg rows/window ≈ {n_rows/N_WINDOWS:.1f})\")\n",
    "\n",
    "# Create bins (ensure no empty bins by redistributing)\n",
    "bins = np.array_split(df_test, N_WINDOWS)\n",
    "# Remove any empty bins\n",
    "bins = [b for b in bins if len(b) > 0]\n",
    "print(\"Actual non-empty windows:\", len(bins))\n",
    "\n",
    "# Build risk series\n",
    "risk_series = pd.Series([b['label_binary'].mean() for b in bins], name='true_risk')\n",
    "print(\"Risk series length:\", len(risk_series))\n",
    "print(\"Risk series (first 20):\\n\", risk_series.head(20))\n",
    "\n",
    "# If risk_series is too short for lags, reduce lags\n",
    "lags = 3\n",
    "if len(risk_series) <= lags:\n",
    "    lags = max(1, len(risk_series) - 1)\n",
    "    print(f\"Adjusted lags to {lags} because series is short.\")\n",
    "\n",
    "# Build lag features\n",
    "Xf = pd.DataFrame({f'lag{i}': risk_series.shift(i) for i in range(1, lags+1)})\n",
    "Xf = Xf.dropna()\n",
    "yf = risk_series.loc[Xf.index]\n",
    "print(\"Lagged feature matrix shape:\", Xf.shape)\n",
    "\n",
    "# If no data after dropna, abort with message\n",
    "if Xf.shape[0] == 0:\n",
    "    raise ValueError(\"No rows left after creating lag features (increase N_WINDOWS or reduce lags).\")\n",
    "\n",
    "# Train/test split (time series, no shuffle)\n",
    "split_idx = int(len(Xf) * 0.8)\n",
    "if split_idx < 1 or split_idx >= len(Xf):\n",
    "    split_idx = max(1, len(Xf) - 1)\n",
    "print(\"split_idx:\", split_idx, \"total rows:\", len(Xf))\n",
    "\n",
    "Xf_train, Xf_test = Xf.iloc[:split_idx], Xf.iloc[split_idx:]\n",
    "yf_train, yf_test = yf.iloc[:split_idx], yf.iloc[split_idx:]\n",
    "print(\"Train/test sizes:\", Xf_train.shape, Xf_test.shape)\n",
    "\n",
    "# Train model with try/except\n",
    "try:\n",
    "    reg = GradientBoostingRegressor(n_estimators=100, random_state=42)\n",
    "    reg.fit(Xf_train, yf_train)\n",
    "    yp = reg.predict(Xf_test)\n",
    "except Exception as e:\n",
    "    print(\"Error training/predicting with regressor:\")\n",
    "    traceback.print_exc()\n",
    "    raise\n",
    "\n",
    "# Evaluate and plot\n",
    "mse = mean_squared_error(yf_test, yp) # returns MSE by default\n",
    "rmse = np.sqrt(mse) # manually compute RMSE\n",
    "print(f\"Forecast MSE: {mse:.6f}\")\n",
    "print(f\"Forecast RMSE: {rmse:.6f}\")\n",
    "\n",
    "# Save model predictions and true values for debugging\n",
    "out = pd.DataFrame({'true': yf_test.values, 'pred': yp})\n",
    "out_path = raw / 'forecast_debug.csv'\n",
    "out.to_csv(out_path, index=False)\n",
    "print(\"Saved forecast_debug.csv to:\", out_path)\n",
    "\n",
    "plt.figure(figsize=(8,4))\n",
    "plt.plot(range(len(yf_test)), yf_test.values, label='true')\n",
    "plt.plot(range(len(yp)), yp, label='pred')\n",
    "plt.xlabel('window index (test set)')\n",
    "plt.ylabel('disruption risk (fraction of attack flows)')\n",
    "plt.legend()\n",
    "plt.title('True vs Predicted disruption risk (test windows)')\n",
    "plt.grid(True)\n",
    "plt.show()\n",
   ]
  },
