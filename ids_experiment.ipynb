    "# CELL: forecast_eval_fix\n",
    "import numpy as np\n",
    "from pathlib import Path\n",
    "from sklearn.metrics import mean_squared_error  # optional fallback\n",
    "\n",
    "# ensure yf_test and yp exist in workspace; if not, reload them from the saved debug CSV\n",
    "try:\n",
    "    _ = yf_test, yp\n",
    "except NameError:\n",
    "    import pandas as pd\n",
    "    out = pd.read_csv(Path('../data')/'forecast_debug.csv')\n",
    "    yf_test = out['true'].values\n",
    "    yp = out['pred'].values\n",
    "\n",
    "# convert to numpy arrays\n",
    "y_true = np.asarray(yf_test).astype(float).ravel()\n",
    "y_pred = np.asarray(yp).astype(float).ravel()\n",
    "\n",
    "print(\"y_true shape, dtype:\", y_true.shape, y_true.dtype)\n",
    "print(\"y_pred shape, dtype:\", y_pred.shape, y_pred.dtype)\n",
    "\n",
    "# compute RMSE manually (safe for all sklearn versions)\n",
    "mse = np.mean((y_true - y_pred) ** 2)\n",
    "rmse = np.sqrt(mse)\n",
    "print(\"Forecast RMSE (manual):\", rmse)\n",
    "\n",
    "# optional: compute MSE via sklearn if available\n",
    "try:\n",
    "    ms = mean_squared_error(y_true, y_pred)\n",
    "    print(\"Forecast MSE (sklearn):\", ms)\n",
    "except Exception as e:\n",
    "    print(\"Could not call sklearn.mean_squared_error:\", e)\n",
    "\n",
    "# Show first few true vs pred for quick sanity-check\n",
    "print(\"first 10 true vs pred:\")\n",
    "for t, p in list(zip(y_true, y_pred))[:10]:\n",
    "    print(f\"true={t:.4f}  pred={p:.4f}\")\n"
