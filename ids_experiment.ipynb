    "# CELL: convert_arff_txt_to_csv\n",
    "from pathlib import Path\n",
    "import pandas as pd\n",
    "import os\n",
    "\n",
    "raw_dir = Path('../data')   # notebook is in notebooks/ so ../data points to Desktop/ids_project/data\n",
    "if not raw_dir.exists():\n",
    "    raise FileNotFoundError(f\"Data directory not found: {raw_dir.resolve()}\")\n",
    "\n",
    "# Standard NSL-KDD column names\n",
    "columns = [\n",
    "    'duration','protocol_type','service','flag','src_bytes','dst_bytes','land',\n",
    "    'wrong_fragment','urgent','hot','num_failed_logins','logged_in','num_compromised',\n",
    "    'root_shell','su_attempted','num_root','num_file_creations','num_shells',\n",
    "    'num_access_files','num_outbound_cmds','is_host_login','is_guest_login','count',\n",
    "    'srv_count','serror_rate','srv_serror_rate','rerror_rate','srv_rerror_rate',\n",
    "    'same_srv_rate','diff_srv_rate','srv_diff_host_rate','dst_host_count',\n",
    "    'dst_host_srv_count','dst_host_same_srv_rate','dst_host_diff_srv_rate',\n",
    "    'dst_host_same_src_port_rate','dst_host_srv_diff_host_rate','dst_host_serror_rate',\n",
    "    'dst_host_srv_serror_rate','dst_host_rerror_rate','dst_host_srv_rerror_rate','label'\n",
    "]\n",
    "\n",
    "def load_arff(path):\n",
    "    try:\n",
    "        from scipy.io import arff\n",
    "        data, meta = arff.loadarff(str(path))\n",
    "        df = pd.DataFrame(data)\n",
    "        # decode bytes to str\n",
    "        for c in df.columns:\n",
    "            if df[c].dtype == object:\n",
    "                df[c] = df[c].apply(lambda x: x.decode() if isinstance(x, (bytes, bytearray)) else x)\n",
    "        return df\n",
    "    except Exception:\n",
    "        import arff\n",
    "        with open(path, 'r') as f:\n",
    "            dataset = arff.load(f)\n",
    "        df = pd.DataFrame(dataset['data'])\n",
    "        df.columns = [attr[0] for attr in dataset['attributes']]\n",
    "        return df\n",
    "\n",
    "def load_txt(path):\n",
    "    # try common separators\n",
    "    for sep in [',','\\t',' ']:\n",
    "        try:\n",
    "            df = pd.read_csv(path, header=None, sep=sep, engine='python')\n",
    "            return df\n",
    "        except Exception:\n",
    "            pass\n",
    "    # fallback\n",
    "    return pd.read_csv(path, header=None)\n",
    "\n",
    "def find_file(base_name):\n",
    "    for ext in ['.arff','.txt']:\n",
    "        p = raw_dir / (base_name + ext)\n",
    "        if p.exists():\n",
    "            return p\n",
    "    # fallback to any matching file\n",
    "    for p in raw_dir.glob(base_name + '*'):\n",
    "        return p\n",
    "    return None\n",
    "\n",
    "train_path = find_file('KDDTrain+')\n",
    "test_path  = find_file('KDDTest+')\n",
    "if not train_path or not test_path:\n",
    "    raise FileNotFoundError(\"Could not find KDDTrain+ or KDDTest+ in data/. Check filenames.\")\n",
    "\n",
    "print(\"Train file:\", train_path.name)\n",
    "print(\"Test file: \", test_path.name)\n",
    "\n",
    "# load\n",
    "if train_path.suffix.lower() == '.arff':\n",
    "    df_train = load_arff(train_path)\n",
    "else:\n",
    "    df_train = load_txt(train_path)\n",
    "\n",
    "if test_path.suffix.lower() == '.arff':\n",
    "    df_test = load_arff(test_path)\n",
    "else:\n",
    "    df_test = load_txt(test_path)\n",
    "\n",
    "# assign standard column names if columns count matches\n",
    "if df_train.shape[1] == len(columns):\n",
    "    df_train.columns = columns\n",
    "if df_test.shape[1] == len(columns):\n",
    "    df_test.columns = columns\n",
    "\n",
    "# Save to CSV\n",
    "train_csv = raw_dir / 'nsl_kdd_train.csv'\n",
    "test_csv  = raw_dir / 'nsl_kdd_test.csv'\n",
    "df_train.to_csv(train_csv, index=False)\n",
    "df_test.to_csv(test_csv, index=False)\n",
    "print(\"Saved:\", train_csv.name, test_csv.name)\n",
    "print(\"Train shape:\", df_train.shape, \"Test shape:\", df_test.shape)\n"
   ]
