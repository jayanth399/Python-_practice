    "# CELL 2: inspect_and_make_subset\n",
    "from pathlib import Path\n",
    "import pandas as pd\n",
    "\n",
    "raw = Path('../data')\n",
    "train_csv = raw/'nsl_kdd_train.csv'\n",
    "test_csv  = raw/'nsl_kdd_test.csv'\n",
    "\n",
    "print(\"Files exist:\", train_csv.exists(), test_csv.exists())\n",
    "\n",
    "df_train = pd.read_csv(train_csv)\n",
    "df_test  = pd.read_csv(test_csv)\n",
    "print(\"Train shape:\", df_train.shape)\n",
    "print(\"Test shape: \", df_test.shape)\n",
    "display(df_train.head())\n",
    "\n",
    "# Create a small balanced subset for quick runs (adjust sizes if you want)\n",
    "normal = df_train[df_train['label']=='normal']\n",
    "attacks = df_train[df_train['label']!='normal']\n",
    "\n",
    "n_sample = 5000\n",
    "small_normal = normal.sample(n=n_sample, random_state=1) if len(normal) > n_sample else normal\n",
    "small_attacks = attacks.sample(n=n_sample, random_state=1) if len(attacks) > n_sample else attacks\n",
    "small = pd.concat([small_normal, small_attacks]).sample(frac=1, random_state=42).reset_index(drop=True)\n",
    "\n",
    "small_path = raw/'nsl_kdd_train_small.csv'\n",
    "small.to_csv(small_path, index=False)\n",
    "print(\"Saved small subset to:\", small_path)\n",
    "\n"
   ]
